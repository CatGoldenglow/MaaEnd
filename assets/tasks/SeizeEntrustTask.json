{
    "task": [
        {
            "name": "SeizeEntrustTask",
            "label": "$task.SeizeEntrustTask.label",
            "entry": "SeizeEntrustTaskMain",
            "description": "$task.SeizeEntrustTask.description",
            "option": [
                "SeizeEntrustTaskisSwipeMiddle"
            ]
        }
    ],
    "option": {
        "SeizeEntrustTaskisSwipeMiddle": {
            "type": "switch",
            "label": "$task.SeizeEntrustTaskisSwipeMiddle.label",
            "description": "$task.SeizeEntrustTaskisSwipeMiddle.description",
            "cases": [
                {
                    "name": "Yes",
                    "option": [
                        "SeizeEntrustTaskisSwipeALL"
                    ],
                    "pipeline_override": {
                        "SeizeEntrustTaskInDownSwipeUp": {
                            "enabled": true
                        },
                        "SeizeEntrustTaskInUpSwipeDown": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "SeizeEntrustTaskInDownSwipeUp": {
                            "enabled": false
                        },
                        "SeizeEntrustTaskInUpSwipeDown": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "SeizeEntrustTaskisSwipeALL": {
            "type": "switch",
            "label": "$task.SeizeEntrustTaskisSwipeALL.label",
            "description": "$task.SeizeEntrustTaskisSwipeALL.description",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "SeizeEntrustTaskInUpSwipeDown": {
                            "roi": [
                                1246,
                                388,
                                23,
                                40
                            ],
                            "template": "SeizeEntrustTask/InMiddle.png"
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "SeizeEntrustTaskInUpSwipeDown": {
                            "roi": [
                                1245,
                                154,
                                24,
                                37
                            ],
                            "template": "SeizeEntrustTask/InUp.png"
                        },
                        "AndroidSeizeEntrustTaskInUpSwipeDown": {
                            "param": {
                                "roi": [
                                    1245,
                                    173,
                                    24,
                                    37
                                ],
                                "template": "SeizeEntrustTask/InUp.png"
                            }
                        }
                    }
                }
            ]
        },
        "SeizeEntrustTaskSetLowThreshold": {
            "type": "switch",
            "label": "降低阈值，如果出现见到7.31w订单不抢的情况，尝试开启此开关以降低模板阈值到0.94，默认为0.95，注意非必要不要开启！！！",
            "description": "开启此选项后，有极小概率抢非7.31w订单，谨慎开启！！！！！",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "SeizeEntrustTaskFound7.31wMoneyF1": {
                            "threshold": 0.95
                        },
                        "SeizeEntrustTaskFound7.31wMoney": {
                            "threshold": 0.95
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "SeizeEntrustTaskFound7.31wMoneyF1": {
                            "threshold": 0.94
                        },
                        "SeizeEntrustTaskFound7.31wMoney": {
                            "threshold": 0.94
                        }
                    }
                }
            ]
        }
    }
}
